<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup>
    <SignAssembly>True</SignAssembly>
    <AssemblyOriginatorKeyFile>$(MSBuildThisFileDirectory)..\keys\FinalPublicKey.snk</AssemblyOriginatorKeyFile>
    <DelaySign>True</DelaySign>
    <!-- Expose our PublicKey for easy IVT setup in SDK style projects, for legacy projects see libs/ProductInfo.cs --> 
    <PublicKey></PublicKey>
  </PropertyGroup>
  <ItemGroup>
    <FilesToSign Include="$(TargetPath)">
      <Authenticode>Microsoft400</Authenticode>
      <StrongName>StrongName</StrongName>
    </FilesToSign>
  </ItemGroup>  
</Project>
